{
  "name": "tdls-website",
  "version": "1.0.0",
  "description": "Source code for TDLS website.",
  "main": "main.js",
  "scripts": {
    "tdls": "npm run dev-sync-blog && npm run sync-data && next tdls -p 3000",
    "a-i": "npm run dev-sync-blog && npm run sync-data && next blog -p 3100",
    "blog": "concurrently \"next blog -p 3200\" \"npm run dev-watch-blog\"",
    "dev-watch-blog": "cd blog && npm run build:content:watch",
    "dev-sync-blog": "cd blog && npm run build:content",
    "sync-contentful": "node scripts/fetch-contentful.js",
    "sync-schedule": "babel-node scripts/sync-schedule.js",
    "sync-data": "npm run sync-contentful & npm run sync-schedule",
    "tdls-build-sitemap": "cross-env SITE_ROOT=https://tdls.a-i.science node scripts/build-sitemap.js tdls",
    "a-i-build-sitemap": "cross-env SITE_ROOT=https://a-i.science node scripts/build-sitemap.js a-i",
    "blog-build-sitemap": "cross-env SITE_ROOT=https://blog.a-i.science node scripts/build-sitemap.js blog",
    "tdls-build": "npm run dev-sync-blog && npm run sync-data && next build tdls && npm run tdls-build-sitemap",
    "a-i-build": "npm run dev-sync-blog && next build a-i && npm run a-i-build-sitemap",
    "blog-build": "npm run dev-sync-blog && cd blog && next build && cd .. && npm run blog-build-sitemap",
    "tdls-start": "next start tdls",
    "a-i-start": "next start a-i",
    "tdls-export": "npm run tdls-build && next export tdls && cp tdls/root-static/* tdls/out/",
    "a-i-export": "npm run a-i-build && next export a-i && cp a-i/root-static/* a-i/out/",
    "blog-export": "npm run blog-build && cd blog && next export && cp root-static/* out/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdls/tdls.github.io.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tdls/tdls.github.io/issues"
  },
  "homepage": "https://github.com/tdls/tdls.github.io#readme",
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "isomorphic-unfetch": "^3.0.0",
    "moment-timezone": "^0.5.23",
    "next": "^7.0.2",
    "node-sass": "^4.11.0",
    "raw-loader": "^1.0.0",
    "react": "^16.8.1",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.1",
    "react-markdown": "^4.0.6",
    "react-modal-video": "^1.2.1",
    "react-responsive-embed": "^2.1.0",
    "react-slick": "^0.23.2",
    "react-youtube": "^7.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@types/next": "^8.0.0",
    "@types/react": "^16.8.2",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^4.1.0",
    "contentful": "^7.2.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "processmd": "^4.4.2",
    "standard": "^12.0.1",
    "styled-jsx": "^3.2.0",
    "typescript": "^3.3.3"
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
}
